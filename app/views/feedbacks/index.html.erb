<h1>Feedbacks</h1>

<table class="table table-hover">
  <thead>
    <th>Created At</th>
    <th>Device ID</th>
    <th>Device Type</th>
    <th>Audio</th>
  </thead>
  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.created_at %></td>
        <td><%= feedback.device_id %></td>
        <td><%= feedback.device_type %></td>
        <td>
          <span>
            <audio controls>
              <source src=<%= feedback.audio %> type="audio/mpeg">
              audio tag not supported.
            </audio>
          </span>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
